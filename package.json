{
  "name": "sortpythonlib",
  "displayName": "sort python lib",
  "description": "",
  "publisher": "Iuri Viveiros",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sortpythonlib.helloWorld",
        "title": "Sort Python Libs"
      }
    ],
    "menus": {
        "editor/context": [
            {
                "when": "editorTextFocus && resourceLangId == 'python'",
                "command": "sortpythonlib.helloWorld",
                "group": "1_modification"
            }
        ]
    },
    "configuration": {
      "sortpythonlib.categories": {
        "type": "array",
        "default": [],
        "description": "Specify import categories for sorting."
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
